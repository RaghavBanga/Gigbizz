                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:singleLine="true"
                            android:text="PW  (Flipkart@123)"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/pw"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="Supplier Mobile No.(register on Flipkart)"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/supplier_mobile"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:inputType="number"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:singleLine="true"
                            android:text="Supplier City"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/supplier_city"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="Supplier GST Number (GST Number should be Correct)"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/supplier_gst"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:inputType="number"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
