                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/BDE_Name"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:inputType="number"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:singleLine="true"
                            android:text="Team Leader Name"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/team_leader"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:singleLine="true"
                            android:text="Manager Name"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/Manager_Name"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:singleLine="true"
                            android:text="BDE Mobile Number"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/BDE_mob"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:background="@drawable/points_bg"
                            android:ems="10"
                            android:enabled="false"
                            android:hint="Your Answer"
                            android:inputType="number"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:singleLine="true"
                            android:text="BDE Email ID"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/BDE_Email_ID"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:background="@drawable/points_bg"
                            android:ems="10"
                            android:enabled="false"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="10 PRODUCT Shared to Catalogue team  (with MRP/MOP &amp; qnt)  if any issues CALL  7982962652/ WhatsApp Catalog Department 9289119959
\n(आपको नीचे दिए गए फॉर्मेट में कैटलॉग टीम को काम से काम १० प्रोडक्ट की फोटू और प्रोडक्ट डिटेल्स उनके व्हाट्सप नंबर पर या अपने ग्रुप में भेजनी है) शॉप का नाम फॉर्मेट में डालना ना भूले"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/product_shared"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:background="@drawable/points_bg"
                            android:ems="10"
                            android:enabled="false"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="Please Guide For Flipkart Fees 1 (Collection Fees)
\nक्या अपने सेलर को गाइड किया निचे दिए गए ऑप्शन पर क्लिक करे-YES/NO"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/collection_fee"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:background="@drawable/points_bg"
                            android:ems="10"
                            android:enabled="false"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="Please Guide For Flipkart Fees 2 (Fixed Fees)
\nक्या अपने सेलर को गाइड किया निचे दिए गए ऑप्शन पर क्लिक करे-YES/NO "
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/Fixed_Fees"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:background="@drawable/points_bg"
                            android:ems="10"
                            android:enabled="false"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="Please Guide For Flipkart Fees 3 (Shipping Fees)
क्या अपने सेलर को गाइड किया निचे दिए गए ऑप्शन पर क्लिक करे-YES/NO  "
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/shipping_Fees"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:background="@drawable/points_bg"
                            android:ems="10"
                            android:enabled="false"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="Is the seller very interested to sell his product on Flipkart and he wants to list his more products and he wants to manage his account?
PLEASE ASK TO SELLER THEN SELECT YES &amp; NO"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/ignite"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:background="@drawable/points_bg"
                            android:ems="10"
                            android:enabled="false"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Photo"
                        android:textSize="@dimen/_12ssp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="@dimen/_3sdp"
                        android:background="@color/grey" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal">

