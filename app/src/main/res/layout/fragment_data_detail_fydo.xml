<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/page_bg_color"
    android:clickable="true"
    android:orientation="vertical"
    android:paddingBottom="@dimen/_5sdp"
    tools:context=".fragments.datadetailscreen.DataDetailFydoFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/pic"
            android:layout_width="@dimen/_18sdp"
            android:layout_height="@dimen/_18sdp"
            android:layout_margin="@dimen/_5sdp"
            android:src="@drawable/back"
            android:background="?selectableItemBackgroundBorderless"
            app:tint="@color/back_icon_color" />

        <TextView
            android:id="@+id/tittle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text=""
            android:textColor="@color/white"
            android:textSize="@dimen/_10ssp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/_5sdp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Data Collection"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/_3sdp"
            android:background="@color/grey" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="Report submitting for"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/submitted_for"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />


                    </LinearLayout>
<!--                    <LinearLayout-->
<!--                        android:layout_width="match_parent"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:layout_marginTop="@dimen/_10sdp"-->
<!--                        android:orientation="vertical">-->

<!--                        <TextView-->
<!--                            android:layout_width="wrap_content"-->
<!--                            android:layout_height="wrap_content"-->
<!--                            android:layout_marginTop="@dimen/_5sdp"-->
<!--                            android:fontFamily="@font/inter_medium_500"-->
<!--                            android:singleLine="true"-->
<!--                            android:text="Working Location"-->
<!--                            android:textColor="@color/grey1"-->
<!--                            android:textSize="@dimen/_10ssp" />-->

<!--                        <EditText-->
<!--                            android:id="@+id/Working_Location"-->
<!--                            android:layout_width="match_parent"-->
<!--                            android:layout_height="@dimen/_35sdp"-->
<!--                            android:ems="10"-->
<!--                            android:hint="Your Answer"-->
<!--                            android:padding="@dimen/_5sdp" />-->

<!--                    </LinearLayout>-->

                    <LinearLayout
                        android:id="@+id/fse_name_lay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:singleLine="true"
                            android:text="FSE Name"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/fse_name"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:singleLine="true"
                            android:text="FSE Mobile Number"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/fse_mob_no"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/customer_name_lay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="User(Customer) Name"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/User_Customer_Name"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/customer_mob_lay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="User(Customer) Mobile Number\n(Who has registered) जो रजिस्टर किया है."
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/user_customer_mob_number"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:inputType="number"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_medium_500"
                            android:text="Team Leader Name"
                            android:textColor="@color/grey1"
                            android:textSize="@dimen/_11ssp" />

                        <EditText
                            android:id="@+id/team_leader_name"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_35sdp"
                            android:ems="10"
                            android:enabled="false"
                            android:background="@drawable/points_bg"
                            android:inputType="number"
                            android:hint="Your Answer"
                            android:padding="@dimen/_5sdp" />

                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Photo"
                        android:textSize="@dimen/_12ssp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="@dimen/_3sdp"
                        android:background="@color/grey" />


                    <LinearLayout
                        android:id="@+id/pic1_good_upload_lay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/MY_Shop_Screenshot"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:ems="10"
                            android:text="MY Shop Screenshot/Photo"
                            android:textColor="@color/black" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/UPLOAD_Store"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:ems="10"
                            android:text="Upload Store Front Images"
                            android:textColor="@color/black" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/UPLOAD_Bank"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:ems="10"
                            android:text="Upload Bank Details Screenshot Images"
                            android:textColor="@color/black" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/UPLOAD_Profile"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:ems="10"
                            android:text="Upload Profile Screenshoot"
                            android:textColor="@color/black" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


        </ScrollView>

    </LinearLayout>


</LinearLayout>